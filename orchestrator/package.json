{
  "name": "ai-cdr-orchestrator",
  "version": "1.0.0",
  "description": "Orquestrador de IA para agente SDR WhatsApp",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "test": "jest --testPathIgnorePatterns=/integration/",
    "test:watch": "jest --watch --testPathIgnorePatterns=/integration/",
    "test:coverage": "jest --coverage --testPathIgnorePatterns=/integration/",
    "test:integration": "jest src/__tests__/integration --testTimeout=60000",
    "test:all": "jest",
    "lint": "eslint src --ext .ts",
    "type-check": "tsc --noEmit",
    "init-faq": "ts-node src/scripts/init-faq.ts",
    "verify-qdrant": "ts-node src/scripts/verify-qdrant-indexing.ts"
  },
  "keywords": [
    "ai",
    "langchain",
    "chatbot",
    "orchestrator"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@langchain/core": "^0.3.0",
    "@langchain/openai": "^0.2.0",
    "@qdrant/js-client-rest": "^1.9.0",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/node": "^20.10.6",
    "@types/jest": "^29.5.11",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}