# ===========================================
# AI-CDR Environment Configuration
# ===========================================
# Copy this file to .env and fill in the values
# The .env file is loaded automatically by docker-compose

# ===========================================
# OpenAI Configuration (REQUIRED)
# ===========================================
OPENAI_API_KEY=sk-your-openai-api-key-here
OPENAI_MODEL=gpt-4o-mini
OPENAI_TEMPERATURE=0.7
OPENAI_EMBEDDING_MODEL=text-embedding-3-small

# ===========================================
# Orchestrator Configuration
# ===========================================
NODE_ENV=development
PORT=3000
SESSION_TIMEOUT_MINUTES=30
LOG_LEVEL=info

# ===========================================
# Qdrant Configuration
# ===========================================
# Internal URL (used by orchestrator inside Docker network)
QDRANT_URL=http://qdrant:6333
QDRANT_API_KEY=
# Collection names
QDRANT_FAQ_COLLECTION=faq_embeddings
QDRANT_CONVERSATIONS_COLLECTION=conversation_history
QDRANT_APPOINTMENTS_COLLECTION=appointment_history

# ===========================================
# Backend Configuration (Internal)
# ===========================================
# Used by orchestrator to call backend functions
BACKEND_URL=http://backend:8080

# ===========================================
# RAG Configuration
# ===========================================
RAG_TOP_K=5
RAG_SCORE_THRESHOLD=0.7
RAG_ENABLE_PATIENT_HISTORY=true

# ===========================================
# Memory Configuration
# ===========================================
MEMORY_BUFFER_SIZE=10
MEMORY_AUTO_ARCHIVE=true
MEMORY_ARCHIVE_ON_COMPLETE=true

# ===========================================
# LangChain Configuration
# ===========================================
LANGCHAIN_TRACING_V2=false
LANGCHAIN_ENDPOINT=https://api.smith.langchain.com
LANGCHAIN_API_KEY=
LANGCHAIN_PROJECT=ai-cdr-orchestrator

# ===========================================
# Development/Debug
# ===========================================
DEBUG_MODE=false
VERBOSE_LOGGING=false
